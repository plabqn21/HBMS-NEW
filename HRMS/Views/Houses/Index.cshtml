@model IEnumerable<HRMS.Models.House>

@{
    ViewBag.Title = "Home List";
    Layout = "~/Views/Shared/_AftterLogIn.cshtml";
}

<h2>All House</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HouseNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HouseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
     
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HouseNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HouseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
      
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) 
            @*@Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
